---
import HeadLayout from "./head_layout.astro";

const { title } = Astro.props.frontmatter;
const baseTitle = " - Axew's Living Room";
---

<HeadLayout title={title + baseTitle}>
  <div class="flex justify-center w-full mt-[25px] md:mt-[40px]">
    <div class="prose p-2 max-w-[900px] flex-1">
      <slot />
    </div>
  </div>
</HeadLayout>
